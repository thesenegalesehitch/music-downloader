{
  "name": "musiquedl",
  "version": "1.0.0",
  "description": "Download music from Spotify, Apple Music, and Deezer with full metadata",
  "exports": "./src/freyr.js",
  "type": "module",
  "bin": {
    "musiquedl": "./cli.js"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "node cli.js",
    "start:web": "node simple_server.js",
    "start:prod": "NODE_ENV=production node simple_server.js",
    "start:i": "node cli.js --interactive",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "eslint cli.js src/ simple_server.js",
    "lint:fix": "eslint --fix cli.js src/ simple_server.js"
  },
  "files": [
    "src",
    "conf.json",
    "banner.js"
  ],
  "directories": {
    "lib": "src"
  },
  "keywords": [
    "spotify",
    "apple-music",
    "deezer",
    "music-downloader",
    "m4a",
    "320kbps",
    "lyrics",
    "metadata"
  ],
  "author": {
    "name": "Alexandre Albert Ndour",
    "email": "aa.ndour5@isepat.edu.sn"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thesenegalesehitch/music-downloader.git"
  },
  "bugs": {
    "url": "https://github.com/thesenegalesehitch/music-downloader/issues"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@ffmpeg/core": "^0.11.0",
    "@ffmpeg/ffmpeg": "^0.11.0",
    "@miraclx/spotify-web-api-node": "^5.1.0",
    "@yujinakayama/apple-music": "^0.4.1",
    "async": "^3.2.4",
    "bluebird": "^3.7.2",
    "cachedir": "^2.3.0",
    "commander": "^11.0.0",
    "compression": "^1.7.4",
    "conf": "^12.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "country-data": "0.0.31",
    "dotenv": "^17.3.1",
    "es-main": "^1.0.2",
    "express": "^4.18.1",
    "express-rate-limit": "^7.4.0",
    "file-type": "^19.0.0",
    "filenamify": "^6.0.0",
    "got": "^13.0.0",
    "helmet": "^7.1.0",
    "hh-mm-ss": "^1.2.0",
    "html-entities": "^2.3.3",
    "isbinaryfile": "^5.0.0",
    "libxget": "^0.11.0",
    "lodash.merge": "^4.6.2",
    "lodash.mergewith": "^4.6.2",
    "lodash.sortby": "^4.7.0",
    "merge2": "^1.4.1",
    "minimatch": "^9.0.0",
    "mkdirp": "^3.0.0",
    "node-cache": "^5.1.2",
    "node-html-parser": "^7.0.2",
    "open": "^10.0.0",
    "pretty-ms": "^9.0.0",
    "public-ip": "^6.0.1",
    "spotify-uri": "^4.0.0",
    "stringd": "^2.2.0",
    "stringd-colors": "^1.10.0",
    "xbytes": "^1.8.0",
    "xprogress": "^0.20.0",
    "youtube-dl-exec": "^3.1.1",
    "yt-search": "^2.10.3"
  },
  "devDependencies": {
    "eslint": "8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "5.2.1",
    "prettier": "3.3.3"
  },
  "overrides": {
    "public-ip": {
      "got": "13.0.0"
    },
    "yt-search": {
      "node-fzf": {
        ".": "0.11.0",
        "cli-color": "2.0.4",
        "string-width": "7.2.0"
      }
    },
    "@yujinakayama/apple-music": {
      "axios": "1.7.2"
    },
    "@ffmpeg/ffmpeg": {
      "node-fetch": "2.7.0"
    }
  },
  "resolutions": {
    "@yujinakayama/apple-music/axios": "1.7.2",
    "@ffmpeg/ffmpeg/node-fetch": "2.7.0",
    "public-ip/got": "13.0.0",
    "yt-search/node-fzf": "0.11.0",
    "yt-search/node-fzf/cli-color": "2.0.4",
    "yt-search/node-fzf/string-width": "7.2.0"
  }
}
